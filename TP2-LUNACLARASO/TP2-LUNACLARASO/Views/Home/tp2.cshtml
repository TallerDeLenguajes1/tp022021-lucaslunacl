<h1>DATOS DEL EMPLEADO -- 2021</h1>

@{
    if (ViewBag.empleado != null)
    {
        <p><bold>Nombre:</bold>@ViewBag.empleado.nombre</p>
        <p><bold>Apellido:</bold>@ViewBag.empleado.apellido</p>
        <p><bold>Salario:</bold>@ViewBag.empleado.salario</p>
        <p><bold>Antiguedad:</bold>@ViewBag.empleado.antiguedad</p>


            }
            else
            {
                <p><bold>@ViewBag.mensaje</bold></p>
            }
}
